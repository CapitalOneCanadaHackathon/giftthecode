@model Indspire.Soaring.Engagement.ViewModels.BulkCreateViewModel
@{
    ViewData["Title"] = "Error Creating Bulk Users";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div>
    <h1>@ViewData["Title"]</h1>
    <p>@Model.ErrorMessage</p>
    <p>@Model.AmountCreated/@Model.Amount users created.</p>
    <a asp-action="BulkCreate">Try again?</a>
</div>